{
    "name": "hanaboso/data-grid",
    "description": "Data grid",
    "license": "Apache-2.0",
    "version": "2.2.5",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.14",
        "doctrine/orm": "^2.14",
        "hanaboso/utils": "^1.2"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\DataGrid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DataGridTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
